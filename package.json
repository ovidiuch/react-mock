{
  "name": "react-mock",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint '**/*.js' --quiet",
    "test": "jest --config .jest/config.js",
    "test:watch": "yarn test --watch",
    "build": "yarn babel-node ./scripts/build.js",
    "copy-npmignore": "yarn babel-node ./scripts/copy-npmignore.js",
    "new-package": "yarn babel-node ./scripts/new-package.js && yarn copy-npmignore",
    "release:check": "yarn flow && yarn lint && yarn test",
    "release:prepare": "yarn copy-npmignore && yarn build",
    "release:lerna": "lerna publish --registry=https://registry.npmjs.org/",
    "release": "yarn release:check && yarn release:prepare && yarn release:lerna"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@skidding/async-retry": "^1.0.2",
    "async-until": "^1.2.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "chalk": "^2.4.1",
    "child-process-promise": "^2.2.1",
    "codecov": "^3.1.0",
    "cpy": "^7.0.1",
    "eslint": "^5.6.0",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.81.0",
    "flow-typed": "^2.5.1",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "inquirer": "^6.2.0",
    "jest": "^23.6.0",
    "lerna": "^3.4.0",
    "lodash": "^4.17.11",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.5.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "yargs": "^12.0.2"
  }
}
